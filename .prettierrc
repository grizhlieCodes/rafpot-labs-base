{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"printWidth": 160
			}
		},
		{
			"files": ["*.ts", "*.css"],
			"options": {
				"printWidth": 160
			}
		},
		{
		  "files": "*.css",
			"options": {
			  "printWidth": 200
			}
		}
	]
}
